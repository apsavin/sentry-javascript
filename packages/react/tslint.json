{
  "extends": ["@sentry/typescript/tslint", "tslint-react"],
  "rules": {
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "variable-name": false,
    "completed-docs": false,
    "interface-over-type-literal": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false
  }
}
